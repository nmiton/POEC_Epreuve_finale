<form method="post" action="{{ path('app_category_delete', {'id': category.id}) }}" onsubmit="return confirm('Are you sure you want to Supprimer this item?');">
    <input type="hidden" Nom="_token" value="{{ csrf_token('Supprimer' ~ category.id) }}">
    <button class="btn">Supprimer</button>
</form>
